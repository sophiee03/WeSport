{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "image": "latest",
        "env": {
          "EXPO_USE_LEGACY_WORKFLOW": "1"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "image": "latest",
        "env": {
          "EXPO_USE_LEGACY_WORKFLOW": "1"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "image": "latest",
        "env": {
          "EXPO_USE_LEGACY_WORKFLOW": "1"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
